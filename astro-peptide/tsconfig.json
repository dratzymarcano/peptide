{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "skipLibCheck": true,
    "strict": true,
    "types": ["astro/client", "react", "react-dom"]
  },
  "include": ["src/**/*", "env.d.ts", ".astro/types.d.ts"]
}
